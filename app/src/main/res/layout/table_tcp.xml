<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lvTemplate"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/txtTcpId"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:text="id"
        android:textSize="20sp"
        android:textColor="#000" />

    <TextView
        android:id="@+id/txtTcpPsrc"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:text="psrc"
        android:textSize="20sp"
        android:textColor="#000" />

    <TextView
        android:id="@+id/txtTcpPdest"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:textSize="20sp"
        android:text="pdest"
        android:textColor="#000" />

    <TextView
        android:id="@+id/txtTcpNseq"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:text="nseq"
        android:textSize="20sp"
        android:textColor="#000" />

    <TextView
        android:id="@+id/txtTcpNack"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:text="nack"
        android:textSize="20sp"
        android:textColor="#000" />

    <TextView
        android:id="@+id/txtTcpChecksum"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:textSize="20sp"
        android:text="checksum"
        android:textColor="#000" />

    <TextView
        android:id="@+id/txtTcpUrp"
        android:layout_width="100dp"
        android:textSize="20sp"
        android:layout_height="wrap_content"
        android:layout_margin="1dp"
        android:text="urp"
        android:textColor="#000" />

</LinearLayout>